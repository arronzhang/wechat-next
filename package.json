{
  "name": "wechat-next",
  "version": "0.1.0",
  "description": "Wechat api",
  "main": "index.js",
  "license": "MIT",
  "jest": {
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test"
    }
  },
  "scripts": {
    "eslint": "eslint *.js --format codeframe \"{lib,test}/**/*.{js,ts}\"",
    "prettier": "prettier --write *.js \"{lib,test}/**/*.{js,ts}\"",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test-travis": "npm run eslint && jest --coverage",
    "test": "npm run eslint && jest"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.16.0",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "prettier": "^1.16.4"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "form-data": "^2.3.3"
  }
}
