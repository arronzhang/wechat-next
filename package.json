{
  "name": "wechat-next",
  "version": "0.4.2",
  "description": "Wechat api",
  "main": "index.js",
  "license": "MIT",
  "jest": {
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test"
    }
  },
  "scripts": {
    "eslint": "eslint *.js --format codeframe \"{lib,test}/**/*.js\"",
    "sish": "ssh -R wechat-test:80:localhost:3000 ssi.sh",
    "format": "prettier --write *.js \"{lib,test}/**/*.{js,ts}\"",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test-travis": "npm run eslint && jest --coverage --collectCoverageFrom=lib/**/*.js",
    "test-typings": "ts-node test/ts/index.ts",
    "test": "npm run eslint && jest"
  },
  "types": "index.d.ts",
  "devDependencies": {
    "axios-mock-adapter": "^1.17.0",
    "body-parser": "^1.19.0",
    "coveralls": "^3.0.7",
    "eslint": "^6.6.0",
    "eslint-plugin-prettier": "^3.1.1",
    "express": "^4.17.1",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "koa": "^2.11.0",
    "koa-body": "^4.1.1",
    "koa-bodyparser": "^4.2.1",
    "koa-xml-body": "^2.1.0",
    "prettier": "^2.1.2",
    "supertest": "^4.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "form-data": "^2.5.1",
    "json-bigint": "^0.3.0",
    "xml-js": "^1.6.11"
  }
}
